{
  "name": "quick-run-debug-buttons",
  "displayName": "Quick Run and Debug Buttons",
  "description": "Quick Run and Debug Buttons",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "icon": "$(debug-start)",
  "contributes": {
    "commands": [
      {
        "command": "quick-run-debug-buttons.debugButton",
        "title": "Start Debugging",
        "icon": "$(debug-start)"
      },
      {
        "command": "quick-run-debug-buttons.runButton",
        "title": "Start Without Debugging",
        "icon": "$(play)"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "quick-run-debug-buttons.debugButton",
          "group": "navigation"
        },
        {
          "command": "quick-run-debug-buttons.runButton",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
